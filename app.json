{
  "expo": {
    "name": "أذكار المسلم",
    "slug": "azkarmuslim",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/hello.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/photo_2024-03-04_20-44-09.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hello.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.azkarMusli.omar"
    },
    "web": {
      "favicon": "./assets/hello.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/path/to/mySound.wav",
            "./local/path/to/myOtherSound.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e38d0459-53bd-43ab-97d2-e4f669792e2a"
      }
    }
  }
}
